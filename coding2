#include<iostream>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int i=1;
      int a=1;
    while(i<=n)
    {
          
        int space=n-i;

        while(space)
        {
            cout<<" ";
            space--;
        }
        int j=1;
        while(j<=i)
        {
            cout<<a;
            a++;
            j++;
        }
        
        cout<<endl;
        i++;
    }
}



//   1
//   121
//  12321
// 1234321



#include<iostream>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int i=1;
     
    while(i<=n)
    {
          
        int space=n-i;
             int a=1;
        while(space)
        {
            cout<<" ";
            space--;
        }
        int j=1;
        while(j<=i)
        {
            cout<<a;
            a++;
            j++;
        }
        
        int k=i-1;
        while(k>0)
        {
            cout<<k;
            k--;
        }
        cout<<endl;
        i++;
    }
}


#include<iostream>
using namespace std;

int main() {
    int n;
    cin >> n;  // Input the number of rows
    
    int i = n;
    while (i >= 1) {
        // Print increasing numbers from 1 to i
        int j = 1;
        while (j <= i) {
            cout << j;
            j++;
        }

        // Print middle spaces or stars (two spaces or one star)
        if (i == n) {
            cout << " ";  // First row with one space
        } else {
            cout << "* ";  // Other rows with a star and space
        }

        // Print decreasing numbers from i to 1
        j = i;
        while (j >= 1) {
            cout << j;
            j--;
        }

        cout << endl;  // Move to the next row
        i--;  // Decrease the row count
    }

    return 0;
}


