#include<iostream>
using namespace std;
int main()
{
    cout<<"hello world"<<endl;
    cout<<"hello smai"<<'\n';
    cout<<"hello will";
    cout<<"\n";
    cout<<1;
}


#include<iostream>
using namespace std;
int main()
{
    int a=123;
    cout<<a<<endl;
    
    char b='v';
    cout<<b<<endl;
    
    float f=1.2;
    cout<<f<<endl;
    
    double d=1.23;
    cout<< d <<endl;
    
    int size=sizeof(a);//in bytes size
    cout<<"size of a is :"<<size<<endl;
}
